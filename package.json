{
  "name": "reuse-net",
  "version": "1.0.0",
  "description": "Community-based waste exchange network",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db": "ts-node -r tsconfig-paths/register src/infrastructure/database/cli.ts",
    "db:migrate": "npm run db migrate",
    "db:seed": "npm run db seed",
    "db:reset": "npm run db reset",
    "db:status": "npm run db status"
  },
  "keywords": [
    "reuse",
    "exchange",
    "community",
    "sustainability"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@google/generative-ai": "^0.24.1",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@sendgrid/mail": "^8.1.0",
    "@types/multer": "^2.0.0",
    "auth0": "^5.1.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openid-connect": "^2.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "multer": "^2.0.2",
    "openai": "^4.20.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
